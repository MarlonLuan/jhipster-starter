{
  "name": "jhipster",
  "version": "0.0.1-SNAPSHOT",
  "private": true,
  "description": "Description for JHipster",
  "license": "UNLICENSED",
  "scripts": {
    "build": "npm run webapp:prod --",
    "build-watch": "concurrently 'npm run webapp:build:dev -- --watch'",
    "ci:frontend:build": "npm run webapp:build:$npm_package_config_default_environment",
    "ci:frontend:test": "npm run ci:frontend:build && npm test",
    "clean-www": "rimraf target/classes/static/app/{src,target/}",
    "cleanup": "rimraf target/classes/static/",
    "jest": "jest --coverage --logHeapUsage --maxWorkers=2 --config jest.conf.js",
    "lint": "eslint . --ext .js,.ts",
    "lint:fix": "npm run lint -- --fix",
    "prettier:check": "prettier --check \"{,src/**/,webpack/,.blueprint/**/}*.{md,json,yml,html,cjs,mjs,js,ts,tsx,css,scss}\"",
    "prettier:format": "prettier --write \"{,src/**/,webpack/,.blueprint/**/}*.{md,json,yml,html,cjs,mjs,js,ts,tsx,css,scss}\"",
    "serve": "npm run start --",
    "sonar": "sonar-scanner",
    "start": "ng serve --hmr",
    "start-tls": "npm run webapp:dev-ssl",
    "pretest": "npm run lint",
    "test": "ng test --coverage --log-heap-usage -w=2",
    "test:watch": "npm run test -- --watch",
    "watch": "concurrently npm:start",
    "webapp:build": "npm run clean-www && npm run webapp:build:dev",
    "webapp:build:dev": "ng build --configuration development",
    "webapp:build:prod": "ng build --configuration production",
    "webapp:dev": "ng serve",
    "webapp:dev-ssl": "ng serve --ssl",
    "webapp:dev-verbose": "ng serve --verbose",
    "webapp:prod": "npm run clean-www && npm run webapp:build:prod",
    "webapp:test": "npm run test --"
  },
  "config": {
    "default_environment": "prod"
  },
  "dependencies": {
    "@angular/common": "15.1.5",
    "@angular/compiler": "15.1.5",
    "@angular/core": "15.1.5",
    "@angular/forms": "15.1.5",
    "@angular/localize": "15.1.5",
    "@angular/platform-browser": "15.1.5",
    "@angular/platform-browser-dynamic": "15.1.5",
    "@angular/router": "15.1.5",
    "@fortawesome/angular-fontawesome": "0.12.1",
    "@fortawesome/fontawesome-svg-core": "6.3.0",
    "@fortawesome/free-solid-svg-icons": "6.3.0",
    "@ng-bootstrap/ng-bootstrap": "14.0.1",
    "@ngx-translate/core": "14.0.0",
    "@ngx-translate/http-loader": "7.0.0",
    "@popperjs/core": "2.11.6",
    "bootstrap": "5.2.3",
    "dayjs": "1.11.7",
    "ngx-infinite-scroll": "15.0.0",
    "ngx-webstorage": "11.1.1",
    "rxjs": "7.8.0",
    "sonar-scanner": "3.1.0",
    "tslib": "2.5.0",
    "zone.js": "0.12.0"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "15.0.0",
    "@angular-builders/jest": "15.0.0",
    "@angular-devkit/build-angular": "15.1.6",
    "@angular-eslint/eslint-plugin": "15.2.1",
    "@angular/cli": "15.1.6",
    "@angular/compiler-cli": "15.1.5",
    "@angular/service-worker": "15.1.5",
    "@types/jest": "28.1.8",
    "@types/node": "16.18.12",
    "@typescript-eslint/eslint-plugin": "5.52.0",
    "@typescript-eslint/parser": "5.52.0",
    "browser-sync": "2.27.11",
    "browser-sync-webpack-plugin": "2.3.0",
    "concurrently": "7.6.0",
    "copy-webpack-plugin": "11.0.0",
    "eslint": "8.34.0",
    "eslint-config-prettier": "8.6.0",
    "eslint-webpack-plugin": "4.0.0",
    "folder-hash": "4.0.4",
    "generator-jhipster": "7.9.3",
    "jest": "28.1.3",
    "jest-date-mock": "1.0.8",
    "jest-environment-jsdom": "28.1.3",
    "jest-junit": "15.0.0",
    "jest-preset-angular": "12.2.6",
    "jest-sonar": "0.2.15",
    "merge-jsons-webpack-plugin": "2.0.1",
    "prettier": "2.8.4",
    "prettier-plugin-packagejson": "2.4.3",
    "rimraf": "4.1.2",
    "swagger-ui-dist": "4.15.5",
    "ts-jest": "28.0.8",
    "typescript": "4.9.5",
    "wait-on": "7.0.1",
    "webpack-bundle-analyzer": "4.8.0",
    "webpack-merge": "5.8.0",
    "webpack-notifier": "1.15.0"
  },
  "engines": {
    "node": ">=16.17.0"
  },
  "cacheDirectories": [
    "node_modules"
  ],
  "overrides": {
    "webpack": "5.75.0"
  }
}
